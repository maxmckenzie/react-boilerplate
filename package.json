{
  "name": "full-stack-test",
  "version": "0.0.1",
  "description": "full stack test hitch",
  "main": "dist/main.js",
  "engines": {
    "node": "6.0.0",
    "npm": "3.10.3"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js 'src/**/*spec.jsx'",
    "dev": "concurrently 'npm run test:watch' 'node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --colors'",
    "lint": "eslint src/",
    "test:watch": "npm run test -- --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "author": "Max McKenzie hello@maxmckenzie.uk",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "bluebird": "^3.4.6",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "json-loader": "^0.5.4"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "enzyme": "^2.4.1",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  }
}
